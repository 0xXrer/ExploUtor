{
  "name": "exploitor",
  "displayName": "ExploUtor",
  "description": "VS Code extension for Roblox exploit development with WebSocket execution and MCP integration",
  "version": "0.0.1",
  "publisher": "exploitor",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "exploitor.connect",
        "title": "ExploUtor: Connect to Executor"
      },
      {
        "command": "exploitor.disconnect",
        "title": "ExploUtor: Disconnect"
      },
      {
        "command": "exploitor.executeSelection",
        "title": "ExploUtor: Execute Selection"
      },
      {
        "command": "exploitor.executeFile",
        "title": "ExploUtor: Execute File"
      },
      {
        "command": "exploitor.searchUpvalues",
        "title": "Search Upvalues",
        "icon": "$(search)"
      },
      {
        "command": "exploitor.searchConstants",
        "title": "Search Constants",
        "icon": "$(search)"
      },
      {
        "command": "exploitor.searchScripts",
        "title": "Search Scripts",
        "icon": "$(search)"
      },
      {
        "command": "exploitor.searchModules",
        "title": "Search Modules",
        "icon": "$(search)"
      },
      {
        "command": "exploitor.searchClosures",
        "title": "Search Closures",
        "icon": "$(search)"
      },
      {
        "command": "exploitor.refreshUpvalues",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "exploitor.refreshConstants",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "exploitor.refreshScripts",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "exploitor.refreshModules",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "exploitor.clearRemoteSpy",
        "title": "Clear",
        "icon": "$(clear-all)"
      },
      {
        "command": "exploitor.clearClosureSpy",
        "title": "Clear",
        "icon": "$(clear-all)"
      },
      {
        "command": "exploitor.toggleRemoteSpy",
        "title": "Toggle Remote Spy",
        "icon": "$(eye)"
      },
      {
        "command": "exploitor.toggleClosureSpy",
        "title": "Search Closures",
        "icon": "$(search)"
      },
      {
        "command": "exploitor.readScript",
        "title": "Read Script Source"
      },
      {
        "command": "exploitor.inspectItem",
        "title": "Inspect"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "exploitor",
          "title": "ExploUtor",
          "icon": "media/icons/exploitor.svg"
        }
      ]
    },
    "views": {
      "exploitor": [
        {
          "id": "exploitor.upvalues",
          "name": "Upvalue Scanner"
        },
        {
          "id": "exploitor.constants",
          "name": "Constant Scanner"
        },
        {
          "id": "exploitor.scripts",
          "name": "Script Scanner"
        },
        {
          "id": "exploitor.modules",
          "name": "Module Scanner"
        },
        {
          "id": "exploitor.remoteSpy",
          "name": "Remote Spy"
        },
        {
          "id": "exploitor.closureSpy",
          "name": "Closure Spy"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "exploitor.searchUpvalues",
          "when": "view == exploitor.upvalues",
          "group": "navigation"
        },
        {
          "command": "exploitor.refreshUpvalues",
          "when": "view == exploitor.upvalues",
          "group": "navigation"
        },
        {
          "command": "exploitor.searchConstants",
          "when": "view == exploitor.constants",
          "group": "navigation"
        },
        {
          "command": "exploitor.refreshConstants",
          "when": "view == exploitor.constants",
          "group": "navigation"
        },
        {
          "command": "exploitor.searchScripts",
          "when": "view == exploitor.scripts",
          "group": "navigation"
        },
        {
          "command": "exploitor.refreshScripts",
          "when": "view == exploitor.scripts",
          "group": "navigation"
        },
        {
          "command": "exploitor.searchModules",
          "when": "view == exploitor.modules",
          "group": "navigation"
        },
        {
          "command": "exploitor.refreshModules",
          "when": "view == exploitor.modules",
          "group": "navigation"
        },
        {
          "command": "exploitor.toggleRemoteSpy",
          "when": "view == exploitor.remoteSpy",
          "group": "navigation"
        },
        {
          "command": "exploitor.clearRemoteSpy",
          "when": "view == exploitor.remoteSpy",
          "group": "navigation"
        },
        {
          "command": "exploitor.searchClosures",
          "when": "view == exploitor.closureSpy",
          "group": "navigation"
        },
        {
          "command": "exploitor.clearClosureSpy",
          "when": "view == exploitor.closureSpy",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "exploitor.modifyUpvalue",
          "when": "view == exploitor.upvalues && viewItem == upvalue"
        },
        {
          "command": "exploitor.inspectItem",
          "when": "view == exploitor.upvalues || view == exploitor.constants || view == exploitor.scripts || view == exploitor.modules"
        }
      ]
    },
    "configuration": {
      "title": "ExploUtor",
      "properties": {
        "exploitor.websocket.host": {
          "type": "string",
          "default": "localhost",
          "description": "WebSocket server host"
        },
        "exploitor.websocket.port": {
          "type": "number",
          "default": 8080,
          "description": "WebSocket server port"
        },
        "exploitor.autoConnect": {
          "type": "boolean",
          "default": true,
          "description": "Automatically connect on startup"
        },
        "exploitor.remoteSpy.enabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable Remote Spy on connect"
        },
        "exploitor.closureSpy.enabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable Closure Spy on connect"
        },
        "exploitor.mcp.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable MCP server"
        },
        "exploitor.mcp.port": {
          "type": "number",
          "default": 3000,
          "description": "MCP server port"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "^20.x",
    "@types/vscode": "^1.85.0",
    "@types/ws": "^8.x",
    "@typescript-eslint/eslint-plugin": "^6.x",
    "@typescript-eslint/parser": "^6.x",
    "eslint": "^8.x",
    "ts-loader": "^9.x",
    "typescript": "^5.x",
    "webpack": "^5.x",
    "webpack-cli": "^5.x"
  },
  "dependencies": {
    "ws": "^8.x",
    "@modelcontextprotocol/sdk": "^1.0.0"
  }
}